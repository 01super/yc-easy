{
  "compilerOptions": {
    "moduleResolution": "node",
    "lib": ["DOM"],
    "module": "ESNext",
    "strict": true,
    "target": "ESNext",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "yc-easy": ["src"],
      "yc-easy/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"]
}
